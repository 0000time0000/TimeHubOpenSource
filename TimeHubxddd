local fov = Dev:AddSection({
    Name = "Field of View"
})

fov:AddSlider({
    Name = "FOV",
    Min = 0,
    Max = 120,
    Default = 70,
    Color = Color3.fromRGB(255,255,255),
    Increment = 1,
    ValueName = "- Your Fov",
    Callback = function(Value)
         game.Workspace.Camera.FieldOfView = (Value)
    end    
})
