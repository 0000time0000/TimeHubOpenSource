local Att = Dev:AddSection({
    Name = "Attachemnts"
})

Att:AddToggle({
	Name = "Main Attachemnts",
	    Icon = "rbxassetid://10849336266",
	Default = false,
	Callback = function(Value)
			game.Players.LocalPlayer.Character.LeftFoot.LeftFootAttachment.Visible = (Value)
			game.Players.LocalPlayer.Character.RightFoot.RightFootAttachment.Visible = (Value)
			game.Players.LocalPlayer.Character.LeftLowerLeg.LeftKneeRigAttachment.Visible = (Value)
			game.Players.LocalPlayer.Character.RightLowerLeg.RightKneeRigAttachment.Visible = (Value)
			game.Players.LocalPlayer.Character.LeftUpperLeg.LeftHipRigAttachment.Visible = (Value)
			game.Players.LocalPlayer.Character.RightUpperLeg.RightHipRigAttachment.Visible = (Value)
			game.Players.LocalPlayer.Character.UpperTorso.BodyFrontAttachment.Visible = (Value)
			game.Players.LocalPlayer.Character.UpperTorso.BodyBackAttachment.Visible = (Value)
			game.Players.LocalPlayer.Character.UpperTorso.LeftCollarAttachment.Visible = (Value)
			game.Players.LocalPlayer.Character.UpperTorso.RightCollarAttachment.Visible = (Value)
			game.Players.LocalPlayer.Character.RightHand.RightGripAttachment.Visible = (Value)
			game.Players.LocalPlayer.Character.LeftHand.LeftGripAttachment.Visible = (Value)
	end    
})

