	Name = "Looped set",
	Default = false,
	Callback = function(lll)
		game.workspace.loop.Value = lll
	end    
})

s:AddButton({
	Name = "Set",
	Callback = function()
if game.workspace.loop.Value == true

then
    while game.workspace.loop.Value == true do
        local sin = math.sin
local cos = math.cos
local rd = math.rad
local cf = {a=CFrame.Angles,n=CFrame.new}
local plr = game:GetService("Players").LocalPlayer; plr.Character.Humanoid:UnequipTools()
local char = plr.Character
local Tools = plr.Backpack:GetChildren()



--boombox grips
Tools[1].Grip = cf.n(game.workspace.x.Value,game.workspace.y.Value,game.workspace.z.Value) * cf.a(rd(game.workspace.RotX.Value),rd(game.workspace.RotY.Value),rd(game.workspace.RotZ.Value))
Tools[1].Parent = char
wait(0.001)
end

    else
        
        
local sin = math.sin
local cos = math.cos
local rd = math.rad
local cf = {a=CFrame.Angles,n=CFrame.new}
local plr = game:GetService("Players").LocalPlayer; plr.Character.Humanoid:UnequipTools()
local char = plr.Character
local Tools = plr.Backpack:GetChildren()



--boombox grips
Tools[1].Grip = cf.n(game.workspace.x.Value,game.workspace.y.Value,game.workspace.z.Value) * cf.a(rd(game.workspace.RotX.Value),rd(game.workspace.RotY.Value),rd(game.workspace.RotZ.Value))
Tools[1].Parent = char
print(x)
end
  	end    
})

s:AddButton({
	Name = "Copy Template",
	Callback = function()
local main = [[
local sin = math.sin
local cos = math.cos
local rd = math.rad
local cf = {a=CFrame.Angles,n=CFrame.new}
local plr = game:GetService("Players").LocalPlayer; plr.Character.Humanoid:UnequipTools()
local char = plr.Character
local Tools = plr.Backpack:GetChildren()

                --   X Y Z ,Rotations X     Y     Z
Tools[1].Grip = cf.n(0,0,0) * cf.a(rd(0),rd(0),rd(0)) 
Tools[1].Parent = char]]

setclipboard(main)
  	end    
})
s:AddButton({
	Name = "Remove mesh",
	Callback = function()
	    local sin = math.sin
local cos = math.cos
local rd = math.rad
local cf = {a=CFrame.Angles,n=CFrame.new}
local plr = game:GetService("Players").LocalPlayer; plr.Character.Humanoid:UnequipTools()
local char = plr.Character
local Tools = plr.Backpack:GetChildren()

                --   X Y Z ,Rotations X     Y     Z
Tools[1].Grip = cf.n(0,0,0) * cf.a(rd(0),rd(0),rd(0)) 
Tools[1].Parent = char
Tools[1].Handle.Mesh:Destroy()
Tools[1].Handle.SpecialMesh:Destroy()
  	end    
})
